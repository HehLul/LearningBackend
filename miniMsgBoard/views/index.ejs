    <body>
        <h1><%= title %></h1>
        <div>
            <h2>All Messages</h2>
            <div>
                <% for(i=0; i<messages.length; i++){ %>
                    <p>By user: <%= messages[i].user %></p>
                    <h3><%= messages[i].text %></h3>
                    <small>Posted: <%= messages[i].added%></small>
                <% } %>
            </div>
        </div>
    </body>
</html>